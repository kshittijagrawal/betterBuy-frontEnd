lex-wrap: wrap; */\n  /* flex-flow: row wrap; */\n  flex-direction: row;\n}\n.productImg {\n  height: 500px;\n\n  position: relative;\n  width: 500px;\n  bottom: 0;\n  /* left: 10%; */\n  border-radius: 5px;\n}\n.product {\n  /* display: flex; */\n  justify-content: center;\n  text-align: center;\n  /* border: 2px ridge cyan; */\n  /* background-color: indigo; */\n  padding: 2px 2px;\n  margin: 30px 30px;\n  /* margin-bottom: 5px; */\n  position: relative;\n}\ndiv.product:hover {\n  z-index: 1;\n  box-shadow: 2px 2px 2px 2px rgb(33, 30, 30);\n  border-radius: 5px;\n  /* cursor: pointer; */\n}\n.product {\n  border-radius: 5px;\n  border: 1px ridge cyan;\n  background-color: aqua;\n\n  /* background-color: indigo; */\n  /* padding: 2px 2px; */\n  /* margin: 2px 2px; */\n  /* margin-bottom: 5px; */\n  /* position: relative; */\n  /* bottom: 0; */\n  /* top: 30px; */\n}\n.btn {\n  color: aliceblue;\n  background-color: black;\n  border-radius: 2px;\n  margin: 2px 2px;\n  box-shadow: 2px 2px grey;\n  opacity: 0.1;\n  align-self: center;\n  justify-self: center;\n  /* height: 80%; */\n}\nbutton.btn:hover {\n  cursor: pointer;\n  background-color: grey;\n  /* color: red; */\n  /* box-sizing: border-box; */\n  /* box-shadow: 1px 1px cyan; */\n  opacity: 0.5;\n}\n.detBtn {\n  color: aliceblue;\n  background-color: black;\n  border-radius: 2px;\n  margin: 2px 2px;\n  box-shadow: 2px 2px grey;\n  /* opacity: 0.1; */\n  /* height: 80%; */\n}\nbutton.detBtn:hover {\n  cursor: pointer;\n  background-color: green;\n  /* color: red; */\n  /* box-sizing: border-box; */\n  box-shadow: 1px 1px cyan;\n  /* opacity: 0.5; */\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}