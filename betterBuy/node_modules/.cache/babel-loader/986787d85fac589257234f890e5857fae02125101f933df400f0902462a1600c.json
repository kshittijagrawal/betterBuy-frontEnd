{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  name: \"productView\",\n  data() {\n    return {\n      isDetails: true,\n      productId: \"\",\n      showProd: true,\n      product: {},\n      merchantId: \"\",\n      userId: localStorage.getItem(\"email\")\n    };\n  },\n  async mounted() {\n    this.productId = this.$route.params.productId;\n    this.merchantId = this.$route.params.merchantId;\n    console.log(\"index = \", this.productId);\n    let prod = await axios.get(\"http://10.20.5.20:8082/product/findMerchantProduct/\" + this.productId + \"/\" + this.merchantId).then(res => {\n      this.product = res.data;\n      console.log(this.product);\n    });\n    console.log(prod);\n  },\n  methods: {\n    async addToCart() {\n      await axios.post(`http://10.20.5.20:8083/user/addToCart/${this.userId}`, {\n        productId: this.productId,\n        merchantId: this.merchantId,\n        itemCount: 1\n      });\n      console.log(\"Added one\");\n      this.$router.push(\"/products/cartview\");\n    },\n    details() {\n      console.log(\"button clicked...\");\n    }\n  }\n};","map":{"version":3,"mappings":";AA+CA;AACA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACA;IACA;IACA;IACAC;IACA,uBACAC,IACA,wDACA,iBACA,MACA,gBACA,CACAC;MACA;MACAF;IACA;IACAA;EACA;EACAG;IACA;MACA;QACAR;QACAG;QACAM;MACA;MACAJ;MACA;IACA;IACAK;MACAL;IACA;EACA;AACA","names":["name","data","isDetails","productId","showProd","product","merchantId","userId","console","get","then","methods","itemCount","details"],"sourceRoot":"src/components","sources":["ProductDescription.vue"],"sourcesContent":["<template>\n  <div class=\"productDetails\">\n    <div class=\"nav\">\n      <div class=\"brand-logo\">\n        <img src=\"../assets/logo.png\" alt=\"\" />\n      </div>\n      <div class=\"heading\">\n        <h1>Product Details</h1>\n      </div>\n      <div class=\"nav-items\">\n        <a href=\"/userview\"><img src=\"../assets/user.png\" alt=\"\" /></a>\n        <a href=\"/products/cartview\"><img src=\"../assets/cart.png\" alt=\"\" /></a>\n      </div>\n    </div>\n    <section v-if=\"showProd\" class=\"product-details\">\n      <div class=\"product-image\">\n        <img :src=\"this.product.image\" alt=\"img not available\" />\n      </div>\n\n      <div class=\"details\">\n        <div class=\"product-brand\">\n          <h3>{{ product.brand }}</h3>\n        </div>\n        <div class=\"product-name\">\n          <p>{{ product.productName }}</p>\n        </div>\n        <div class=\"product-usp\">\n          <p>{{ product.productUsp }}</p>\n        </div>\n        <div class=\"product-price\">\n          <p>{{ product.price }}</p>\n        </div>\n        <div class=\"product-description\">\n          <p>{{ product.description }}</p>\n        </div>\n        <div class=\"product-review\">\n          <p>{{ product.productReview }}</p>\n        </div>\n        <div class=\"atc-button\">\n          <button @click=\"addToCart\" type=\"button\">Add to Cart</button>\n        </div>\n      </div>\n    </section>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n  name: \"productView\",\n  data() {\n    return {\n      isDetails: true,\n      productId: \"\",\n      showProd: true,\n      product: {},\n      merchantId: \"\",\n      userId: localStorage.getItem(\"email\"),\n    };\n  },\n  async mounted() {\n    this.productId = this.$route.params.productId;\n    this.merchantId = this.$route.params.merchantId;\n    console.log(\"index = \", this.productId);\n    let prod = await axios\n      .get(\n        \"http://10.20.5.20:8082/product/findMerchantProduct/\" +\n          this.productId +\n          \"/\" +\n          this.merchantId\n      )\n      .then((res) => {\n        this.product = res.data;\n        console.log(this.product);\n      });\n    console.log(prod);\n  },\n  methods: {\n    async addToCart() {\n      await axios.post(`http://10.20.5.20:8083/user/addToCart/${this.userId}`, {\n        productId: this.productId,\n        merchantId: this.merchantId,\n        itemCount: 1,\n      });\n      console.log(\"Added one\");\n      this.$router.push(\"/products/cartview\");\n    },\n    details() {\n      console.log(\"button clicked...\");\n    },\n  },\n};\n</script>\n<style scoped>\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n  font-family: \"roboto\", sans-serif;\n}\n\n.nav {\n  padding: 0.8%;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  position: sticky;\n  top: 0;\n  left: 0;\n  width: 100%;\n  background: #f5f5f5;\n  z-index: 9;\n  border-bottom: 1px solid #d1d1d1;\n}\n\n.brand-logo img {\n  height: 60px;\n  transition: 1s;\n}\n\n.brand-logo :hover {\n  transform: rotate(180deg);\n}\n\n.nav-items {\n  display: flex;\n  align-items: center;\n}\n.nav-items a {\n  margin-left: 20px;\n  display: flex;\n  align-items: center;\n}\n\n.nav-items a img {\n  width: 30px;\n}\n\n.nav-items a:hover {\n  top: -10px;\n}\n\nh1 {\n  text-align: center;\n}\n\n.product-details {\n  width: 100%;\n  padding: 60px 10vw;\n  display: flex;\n  justify-content: space-between;\n}\n\n.product-image {\n  position: absolute;\n  bottom: 20px;\n  left: 50%;\n  transform: translateX(-50%);\n  width: 90%;\n  background: #fff;\n  border-radius: 5px;\n  display: grid;\n  grid-template-columns: repeat(4, 1fr);\n  height: 100px;\n  grid-gap: 10px;\n  padding: 10px;\n}\n\n.product-image img {\n  width: 100%;\n  height: 80px;\n  object-fit: cover;\n  cursor: pointer;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}