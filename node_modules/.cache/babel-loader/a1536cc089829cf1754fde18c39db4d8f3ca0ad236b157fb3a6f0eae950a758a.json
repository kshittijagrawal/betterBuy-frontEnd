{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  data() {\n    return {\n      cart: [],\n      itemCount: \"\",\n      email: localStorage.getItem(\"email\")\n    };\n  },\n  watch: {\n    itemCount() {\n      return this.itemCount;\n    }\n  },\n  props: [\"SingleCart\"],\n  methods: {\n    handleAdd() {\n      axios.post(`http://10.20.5.20:8083/user/addToCart/${this.email}`, {\n        productId: this.SingleCart.productId,\n        merchantId: this.SingleCart.merchantId,\n        itemCount: 1\n      });\n      this.itemCount++;\n    },\n    handleSubtract() {\n      if (!this.itemCount == 0) {\n        axios.post(`http://10.20.5.20:8083/user/removeFromCart/george@gmail.com`, {\n          productId: this.SingleCart.productId,\n          merchantId: this.SingleCart.merchantId,\n          itemCount: 1\n        });\n        this.itemCount--;\n      } else {\n        axios.delete(`http://10.20.5.20:8083/user/deleteFromCart/${this.SingleCart.productId}/${this.SingleCart.merchantId}/${this.email}/`);\n      }\n    },\n    handleRemove() {\n      axios.delete(`http://10.20.5.20:8083/user/deleteFromCart/${this.SingleCart.productId}/${this.SingleCart.merchantId}/${this.email}/`);\n      console.log(this.SingleCart);\n      window.location.reload();\n    },\n    handleBuy() {\n      axios.post(`http://10.20.5.20:8083/user/checkOut/george@gmail.com`);\n    }\n  },\n  created() {\n    this.itemCount = this.SingleCart.itemCount;\n  }\n  //   async created() {\n  //     this.cart = await axios\n  //       .get(\"http://localhost:8083/user/viewCart/george@gmail.com\")\n  //       .then((res) => (this.cart = res.data));\n  //     // console.log(this.cart.cartResponse[0]);\n\n  //     this.cart = this.cart.cartResponse;\n  //     console.log(this.cart[0].image);\n  //   },\n};","map":{"version":3,"mappings":"AAkBA;AACA;EACAA;IACA;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACAF;MACA;IACA;EACA;EACAG;EACAC;IACAC;MACAC;QACAC;QACAC;QACAR;MACA;MACA;IACA;IACAS;MACA;QACAH,WACA,+DACA;UACAC;UACAC;UACAR;QACA,EACA;QACA;MACA;QACAM,aACA,uHACA;MACA;IACA;IACAI;MACAJ,aACA,uHACA;MACAK;MACAC;IACA;IACAC;MACAP;IACA;EACA;EACAQ;IACA;EACA;EACA;EACA;EACA;EACA;EACA;;EAEA;EACA;EACA;AACA","names":["data","cart","itemCount","email","watch","props","methods","handleAdd","axios","productId","merchantId","handleSubtract","handleRemove","console","window","handleBuy","created"],"sourceRoot":"src/components","sources":["CartView.vue"],"sourcesContent":["<template>\n  <div class=\"CartItem\">\n    <div class=\"paddingDiv\">\n      <img :src=\"SingleCart.image\" alt=\"imagenotfound\" />\n      <div class=\"details\">\n        <h3>{{}} {{ SingleCart.productName }}</h3>\n        <p>Price: {{ SingleCart.price }}</p>\n        <p>Number: {{ itemCount }}</p>\n        <p>Merchant: {{ SingleCart.merchantName }}</p>\n      </div>\n      <button class=\"button-38\" @click=\"handleSubtract\">Subtract Count</button\n      ><button class=\"button-38\" @click=\"handleAdd\">Add Count</button>\n      <button class=\"button-38\" @click=\"handleRemove\">Remove Item</button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      cart: [],\n      itemCount: \"\",\n      email: localStorage.getItem(\"email\"),\n    };\n  },\n  watch: {\n    itemCount() {\n      return this.itemCount;\n    },\n  },\n  props: [\"SingleCart\"],\n  methods: {\n    handleAdd() {\n      axios.post(`http://10.20.5.20:8083/user/addToCart/${this.email}`, {\n        productId: this.SingleCart.productId,\n        merchantId: this.SingleCart.merchantId,\n        itemCount: 1,\n      });\n      this.itemCount++;\n    },\n    handleSubtract() {\n      if (!this.itemCount == 0) {\n        axios.post(\n          `http://10.20.5.20:8083/user/removeFromCart/george@gmail.com`,\n          {\n            productId: this.SingleCart.productId,\n            merchantId: this.SingleCart.merchantId,\n            itemCount: 1,\n          }\n        );\n        this.itemCount--;\n      } else {\n        axios.delete(\n          `http://10.20.5.20:8083/user/deleteFromCart/${this.SingleCart.productId}/${this.SingleCart.merchantId}/${this.email}/`\n        );\n      }\n    },\n    handleRemove() {\n      axios.delete(\n        `http://10.20.5.20:8083/user/deleteFromCart/${this.SingleCart.productId}/${this.SingleCart.merchantId}/${this.email}/`\n      );\n      console.log(this.SingleCart);\n      window.location.reload();\n    },\n    handleBuy() {\n      axios.post(`http://10.20.5.20:8083/user/checkOut/george@gmail.com`);\n    },\n  },\n  created() {\n    this.itemCount = this.SingleCart.itemCount;\n  },\n  //   async created() {\n  //     this.cart = await axios\n  //       .get(\"http://localhost:8083/user/viewCart/george@gmail.com\")\n  //       .then((res) => (this.cart = res.data));\n  //     // console.log(this.cart.cartResponse[0]);\n\n  //     this.cart = this.cart.cartResponse;\n  //     console.log(this.cart[0].image);\n  //   },\n};\n</script>\n<style scoped>\n/* .CartItem { */\n/* width: 100%; */\n/* margin: 0%; */\n/* } */\n/* .CartItem {\n  border: 2px solid black;\n  border-radius: 5%;\n  width: 27%;\n}\n\nimg {\n  width: 200px;\n  height: 300px;\n}\n.paddingdiv {\n  padding: 5%;\n}\n.CartItem button {\n  display: flex;\n  flex-direction: column;\n  /* width: 100%; */\n.CartItem {\n  width: 300px;\n  height: 400px;\n  padding: 5px;\n  margin: 10px;\n  background-color: aqua;\n  /* margin-left: 200px; */\n  /* margin-bottom: 50px; */\n  /* margin-top: 10px; */\n  /* padding: 10px; */\n  border: 2px solid black;\n  border-radius: 10px;\n  /* border-radius: 5px;  */\n}\n.CartItem img {\n  height: 200px;\n  width: 300px;\n}\n\n/* CSS */\n.button-38 {\n  background-color: #ffffff;\n  margin: 3px;\n  border: 0;\n  border-radius: 0.5rem;\n  box-sizing: border-box;\n  color: #111827;\n  font-family: \"Inter var\", ui-sans-serif, system-ui, -apple-system, system-ui,\n    \"Segoe UI\", Roboto, \"Helvetica Neue\", Arial, \"Noto Sans\", sans-serif,\n    \"Apple Color Emoji\", \"Segoe UI Emoji\", \"Segoe UI Symbol\", \"Noto Color Emoji\";\n  font-size: 0.875rem;\n  font-weight: 600;\n  line-height: 1.25rem;\n  padding: 0.75rem 1rem;\n  text-align: center;\n  text-decoration: none #d1d5db solid;\n  text-decoration-thickness: auto;\n  box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06);\n  cursor: pointer;\n  user-select: none;\n  -webkit-user-select: none;\n  touch-action: manipulation;\n}\n\n.button-38:hover {\n  background-color: rgb(249, 250, 251);\n}\n\n.button-38:focus {\n  outline: 2px solid transparent;\n  outline-offset: 2px;\n}\n\n.button-38:focus-visible {\n  box-shadow: none;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}