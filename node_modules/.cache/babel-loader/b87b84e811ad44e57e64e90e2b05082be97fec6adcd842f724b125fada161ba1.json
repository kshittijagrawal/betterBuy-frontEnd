n        </div>\n      </form>\n    </div>\n  </template>\n  \n  <script>\n  import axios from \"axios\";\n  export default {\n    name: \"MerchantLogin\",\n    data() {\n      return {\n        email: \"\",\n        password: \"\",\n        show: true,\n      };\n    },\n    methods: {\n      async handleSubmit() {\n        const response = await axios.post(\n          \"http://10.20.5.34:8081/merchant/login/\" + this.email + this.password,\n          {\n            email: this.email,\n            password: this.password,\n          }\n        );\n        console.log(response);\n        if (response.data === true) {\n          localStorage.setItem(\"email\", this.email);\n          // this.show = false;\n          // this.$router.push(\"/merchant/products\");\n        }\n      },\n    },\n  };\n  </script>\n  \n  <style scoped>\n  .signin-page {\n    display: flex;\n    justify-content: center;\n  }\n  \n  form {\n    margin-top: 40px;\n    margin-left: 70px;\n    border: 2px solid black;\n    width: 40%;\n    padding-bottom: 10px;\n  }\n  \n  .name {\n    display: flex;\n    justify-content: space-around;\n  }\n  \n  .bottom {\n    display: flex;\n    flex-wrap: wrap;\n  }\n  \n  #address {\n    width: 70%;\n  }\n  \n  /* .phone, .email, .password, .confirm {\n        float: left;\n    } */\n  \n  .field {\n    padding: 10px;\n  }\n  </style>\n  "]},"metadata":{},"sourceType":"module","externalDependencies":[]}